{
  "extends": "tsconfigs/browser-executable",
  "compilerOptions": {
    "outDir": "build",
    "declaration": true,
    "sourceMap": true,
    "target": "es2021",
    "module": "commonjs",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  },
  "include": ["src/**/*"],
  "exclude": ["src/**/*.test.ts"]
}
